<html lang="en"> <!--<![endif]-->
  <head>
  	<meta charset="utf-8" />

  	<!-- Uncomment to make IE8 render like IE7 -->
  	<!-- <meta http-equiv="X-UA-Compatible" content="IE=7" /> -->

  	<!-- Set the viewport width to device width for mobile -->
  	<!-- <meta name="viewport" content="width=device-width, initial-scale=1.0" /> -->
    <meta name="viewport" content="width=device-width" />

     <% # Our title can be custom set by modifying the @title variable. If missing use default %>
     <title><%= (@title.nil? ? "#{APP_NAME} - #{APP_TITLE}" : "#{h(@title)} - #{APP_NAME}") %></title>
    <!-- Included CSS Files -->     
	<%= stylesheet_link_tag    "application", :media => "all" %>
	<%= javascript_include_tag "vendor/custom.modernizr" %>
	<%= csrf_meta_tags %>  
  </head>

  <body onload="setup();">
		<%= render "layouts/header" %>
		<%= yield %>
</div>
 <!-- Footer --> 
  <footer>          
    <div id="footer-header"></div>                
          <div class="row">
              <div id="footer-copy" class="small-12 large-6 columns">
                  <h5 style="color: white;">&copy; Copyright <%= "#{COMPANY_NAME}" %></h5>
              </div>

              <div id="footer-menu" class="small-12 large-6 columns">
                  <ul class="link-list right">
                    <li><a href="#">Privacy</a></li>
                    <li><a href="#">Contact us</a></li>
                    <li><a href="#">Terms of Service</a></li>
                    <li><a href="#">Register</a></li>
                  </ul>
              </div>

          </div>
          <div class="row">            
              <div id="social-media">
                <!-- Twitter -->
                <a href="http://www.twitter.com/thoughtware/" target="_blank"><img alt="Twitter" border="0" style="none" src="data:image/jpeg;base64, "/></a> 

                <!-- RSS Icon -->
                <a href="http://www.thoughtware.tv/feed/videos/" target="_blank"><img alt="Our RSS Feed" border="0" style="none" src="data:image/jpeg;base64, " /></a>

              </div>

          </div>         
      </div>
  </footer>

  <!-- Included JS Files  -->
  <%= javascript_include_tag "application" %>
  <script type="application/javascript">
    // $(function() { $("textarea").autoResize();})
  </script> 
</body>
</html>